{
  "name": "rainworld-tracker",
  "version": "1.0.0",
  "description": "A tracking application to help de-mystify the hunter save file.",
  "main": "js/background.js",
  "build": {
    "productName": "Rain World Tracker",
    "win": {
      "target": ["portable"]
    },
    "appId": "com.jaclynonacloud.rainworldtracker",
    "mac": {
      "category": "tracker"
    }
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "gh-publish": "electron-builder -p always"
  },
  "postinstall": "electron-builder install-app-deps",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^12.0.2",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.4",
    "electron-updater": "^4.0.6",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-sourcemaps": "^2.6.0",
    "merge-stream": "^1.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "babel-core": "^6.1.21",
    "electron-cli": "^0.2.8"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "author": "Jaclyn Staples",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaclynonacloud/rainworld-tracker.git"
  },
  "keywords": [
    "Rain",
    "World",
    "Tracker",
    "Hunter",
    "jaclynonacloud",
    "Jaclyn",
    "Staples"
  ],
  "bugs": {
    "url": "https://github.com/jaclynonacloud/rainworld-tracker/issues"
  },
  "homepage": "https://github.com/jaclynonacloud/rainworld-tracker#readme"
}
